{
  "editor.fontFamily": "Hasklug Nerd Font Propo",
  // or JetBrainsMonoNL Nerd Font
  "breadcrumbs.enabled": false,
  "explorer.confirmDelete": false,
  "workbench.editorAssociations": {
    "*.js": "default",
    "*.sql": "default",
    "*.db": "default"
  },
  "git.confirmSync": false,
  "workbench.colorTheme": "Dracula Theme",
  "workbench.iconTheme": "symbols",
  "editor.rulers": [
    110
  ],
  "editor.wordWrap": "on",
  "editor.fontSize": 14,
  "editor.lineHeight": 1.5,
  "editor.fontLigatures": true,
  "editor.scrollbar.horizontal": "hidden",
  "explorer.confirmDragAndDrop": false,
  "editor.stickyScroll.enabled": false,
  "editor.minimap.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "window.customTitleBarVisibility": "auto",
  "symbols.hidesExplorerArrows": false,
  "editor.guides.bracketPairs": false,
  "workbench.editor.tabSizingFixedMaxWidth": 100,
  "workbench.editor.wrapTabs": true,
  "window.density.editorTabHeight": "compact",
  "terminal.explorerKind": "external",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 200,
  "terminal.integrated.shellIntegration.history": 15,
  "codesnap-plus.containerPadding": "1em",
  "security.workspace.trust.untrustedFiles": "open",
  "symbols.folders.associations": {
    "controllers": "folder-auth"
  },
  "workbench.startupEditor": "none",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.compactFolders": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codesnap-plus.showLineNumbers": false,
  "codesnap-plus.showWindowControls": false,
  "prettier.tabWidth": 4,
  "workbench.settings.applyToAllProfiles": [
    "prettier.tabWidth"
  ],
  "window.zoomLevel": -1.08,
  "diffEditor.ignoreTrimWhitespace": true,
  "files.autoSave": "off",
  "workbench.panel.defaultLocation": "right",
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.inheritEnv": true,
}